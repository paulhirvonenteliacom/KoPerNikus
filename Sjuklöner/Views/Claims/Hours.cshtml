                            }
                            else {
                                numberOfMinutes[i] = 60 - startMinuteSI;
                                numberOfHours[i] = 7 - parseFloat(startTimeHourSI[i].value) - 1;
                            }
                            numberOfMorningMinutes = numberOfMinutes[i];
                            numberOfMorningHours = numberOfHours[i];
