                                                <div class="col col-md-12 control-label-left-small-padding bold-18">
                                                    Summa, @Model.ClaimCalculations[i].StartDate.AddDays(1).ToShortDateString()
                                                </div>
                                            </div>
                                        </div>
                                    }
                                    else if (i == 0 && Model.ClaimCalculations[i].NumberOfSickDays > 1)
                                    {
                                        <div class="col col-md-4">
                                            <div class="row">
                                                <div class="col col-md-12 control-label-left-small-padding bold-18">
                                                    Summa, @Model.ClaimCalculations[i].StartDate.AddDays(1).ToShortDateString() - @Model.ClaimCalculations[i].EndDate.ToShortDateString()
                                                </div>
                                            </div>
                                        </div>
                                    }
                                    else if (Model.ClaimCalculations[i].NumberOfSickDays == 1)
                                    {
                                        <div class="col col-md-4">
                                            <div class="row">
                                                <div class="col col-md-12 control-label-left-small-padding bold-18">
                                                    Summa, @Model.ClaimCalculations[i].StartDate.ToShortDateString()
                                                </div>
                                            </div>
                                        </div>
                                    }
                                    else
                                    {
                                        <div class="col col-md-4">
                                            <div class="row">
                                                <div class="col col-md-12 control-label-left-small-padding bold-18">
                                                    Summa, @Model.ClaimCalculations[i].StartDate.ToShortDateString() - @Model.ClaimCalculations[i].EndDate.ToShortDateString()
                                                </div>
                                            </div>
                                        </div>
                                    }
                                }
                                <div class="col col-md-6">
                                    <div class="row">
                                        <div class="col col-md-8">
                                            @Html.DisplayFor(m => m.ClaimCalculations[i].CostCalcD15Plus)
                                        </div>
                                        <div class="col col-md-1">
                                            =
                                        </div>
                                        <div class="col col-md-3 text-right bold-18">
                                            @Html.DisplayFor(m => m.ClaimCalculations[i].CostD15Plus) Kr
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <hr class="hr-low-margin" />
                            <br />
                            <br />
                        }
                    }

                    @if (Model.AdjustedNumberOfSickDays <= 14)
                    {
                        <div class="row">
                            <div class="col col-md-4">
                                <div class="row">
                                    <div class="col col-md-12 control-label-left-small-padding form-group-subheading">
                                        Summa hela sjukperioden:
                                    </div>
                                </div>
                            </div>
                            <div class="col col-md-6">
                                <div class="row">
                                    <div class="col col-md-8 calc-padding-top">
                                        @Html.DisplayFor(m => m.TotalCostCalcD1T14) Kr
                                    </div>
                                    <div class="col col-md-1 calc-padding-top">
                                        =
                                    </div>
                                    <div class="col col-md-3 text-right bold-18">
                                        @Html.DisplayFor(m => m.TotalCostD1T14) Kr
                                    </div>
                                </div>
                            </div>
                            <div class="col col-md-2 text-right bold-18">
                                @Html.DisplayFor(m => m.TotalCostD1T14) Kr
                            </div>
                        </div>
                    }
                    else
                    {
                        //more than 14 sickdays
                        <div class="row">
                            <div class="col col-md-4">
                                <div class="row">
                                    <div class="col col-md-12 control-label-left-small-padding form-group-subheading">
                                        Summa hela sjukperioden:
                                    </div>
                                </div>
                            </div>
                            <div class="col col-md-6">
                                <div class="row">
                                    <div class="col col-md-8 calc-padding-top">
                                        @Html.DisplayFor(m => m.TotalCostCalcD1Plus) Kr
                                    </div>
                                    <div class="col col-md-1 calc-padding-top">
                                        =
                                    </div>
                                    <div class="col col-md-3 text-right bold-18">
                                        @Html.DisplayFor(m => m.TotalCostD1Plus) Kr
                                    </div>
                                </div>
                            </div>
                            <div class="col col-md-2 text-right bold-18">
                                @Html.DisplayFor(m => m.TotalCostD1Plus) Kr
                            </div>
                        </div>
                        <br />
                        <br />
                    }

                    @if (Model.DefaultCollectiveAgreement)
                    {
                        <div class="row display-padding5">
                            Defaultvärden för OB- och jourersättningar har använts i beräkningen.@Html.ActionLink("Klicka här ", "Index", "CollectiveAgreements", null, new { @class = "breadcrumb-padding" })för att lägga in aktuella ersättningsbelopp enligt kollektivavtal.
                        </div>
                    }
                </div>
            </div>
        }
    </div>
</body>
